{
  "builds": [
    {
      "src": "app/fall_logic.py",
      "use": "@vercel/python",
      "config": {
        "externalPackages": [
          "torch",
          "torchvision",
          "mediapipe",
          "opencv-python",
          "numpy"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/fall_logic.py"
    }
  ]
}